{
  "compileOnSave": false,
  "compilerOptions": {
    "rootDir": ".",
    "sourceMap": true,
    "declaration": false,
    "moduleResolution": "node",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "importHelpers": true,
    "target": "es2015",
    "module": "esnext",
    "lib": ["es2020", "dom"],
    "skipLibCheck": true,
    "skipDefaultLibCheck": true,
    "baseUrl": ".",
    "paths": {
      "@taro/auth/common": ["libs/auth/common/src/index.ts"],
      "@taro/auth/guards": ["libs/auth/guards/src/index.ts"],
      "@taro/auth/services": ["libs/auth/services/src/index.ts"],
      "@taro/auth/page": ["libs/auth/page/src/index.ts"],
      "@taro/backend/auth": ["libs/backend/auth/src/index.ts"],
      "@taro/backend/cards": ["libs/backend/cards/src/index.ts"],
      "@taro/backend/core": ["libs/backend/core/src/index.ts"],
      "@taro/backend/jwt": ["libs/backend/jwt/src/index.ts"],
      "@taro/backend/spread": ["libs/backend/spread/src/index.ts"],
      "@taro/backend/users": ["libs/backend/users/src/index.ts"],
      "@taro/cards/common": ["libs/cards/common/src/index.ts"],
      "@taro/core": ["libs/core/src/index.ts"],
      "@taro/dashboard/page": ["libs/dashboard/page/src/index.ts"],
      "@taro/spreads/common": ["libs/spreads/common/src/index.ts"],
      "@taro/ui/layout": ["libs/ui/layout/src/index.ts"],
      "@taro/users/common": ["libs/users/common/src/index.ts"],
      "@taro/users/guards": ["libs/users/guards/src/index.ts"],
      "@taro/users/services": ["libs/users/services/src/index.ts"],
      "@taro/web/home/page": ["libs/web/home/page/src/index.ts"]
    }
  },
  "exclude": ["node_modules", "tmp"]
}
